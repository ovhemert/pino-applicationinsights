{
  "name": "pino-applicationinsights",
  "version": "3.0.0-dev",
  "description": "A transport for pino that sends messages to Azure Application Insights",
  "homepage": "https://github.com/ovhemert/pino-applicationinsights",
  "main": "src/index.js",
  "scripts": {
    "dev:cli": "node ./test/debug.js | node ./src/cli.js",
    "lint": "echo \"linting temporarily removed\"",
    "test": "lint && tap test/*.test.js --coverage --100",
    "test:report": "lint && tap test/*.test.js --coverage  --coverage-report=html --100",
    "type-check": "tsc --pretty --noEmit",
    "posttest": "tap --coverage --coverage-report=lcovonly",
    "format:all": "prettier --write .",
    "preinstall": "git config --local core.eol lf && git config --local core.autocrlf false && git config --local blame.ignoreRevsFile .gitblameignore"
  },
  "bin": {
    "pino-applicationinsights": "src/cli.js"
  },
  "keywords": [
    "pino",
    "pino-transport",
    "azure",
    "application",
    "insights",
    "logging"
  ],
  "author": "Osmond van Hemert <hello@ovhemert.dev> (https://ovhemert.dev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ovhemert/pino-applicationinsights.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "@tsconfig/node16-strictest": "^1.0.0",
    "@types/pino-multi-stream": "^5.1.3",
    "@types/prettier": "^2.4.4",
    "@types/pumpify": "^1.4.1",
    "@types/sinon": "^10.0.11",
    "@types/split2": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "pino-multi-stream": "^5.1.1",
    "prettier": "^2.6.2",
    "sinon": "^11.1.2",
    "tap": "^15.0.9",
    "typescript": ">=4.6.3"
  },
  "dependencies": {
    "applicationinsights": "^2.1.4",
    "batch2": "^2.0.0",
    "commander": "^6.2.1",
    "fast-json-parse": "^1.0.3",
    "pumpify": "^2.0.1",
    "split2": "^3.2.2",
    "stream": "0.0.2"
  },
  "volta": {
    "node": "16.14.2",
    "npm": "8.5.0"
  }
}
